{
  "name": "@ticatec/i18n",
  "version": "0.0.1",
  "description": "A lightweight frontend internationalization (i18n) solution that provides multi-language support for applications.",
  "main": "./dist/index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "publish:public": "npm publish --access public"
  },
  "keywords": [
    "i18n", "languages", "internationalization", "svelte", "react"
  ],
  "repository": {
    "type": "https",
    "url": "https://github.com/ticatec/i18n.git"
  },
  "bugs": {
    "url": "https://github.com/ticatec/i18n/issues"
  },
  "homepage": "https://github.com/ticatec/i18n",
  "author": "Henry Feng",
  "license": "MIT",
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./I18nContext": {
      "types": "./dist/i18n/index.d.ts",
      "import": "./dist/i18n/index.js"
    }
  },
  "private": false,
  "dependencies": {
  }
}
